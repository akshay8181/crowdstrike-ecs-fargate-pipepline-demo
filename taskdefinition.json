{
    "family": "java_repo", 
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "java", 
            "image": "#AWSACCOUNTID#.dkr.ecr.us-east-1.amazonaws.com/java_repo:latest", 
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8443,
                    "hostPort": 8443,
                    "protocol": "tcp"
                }
            ],
            "readonlyRootFilesystem": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-fargate-pipeline-demo-app",
                    "awslogs-region": "us-east-1",
                    "awslogs-create-group": "true",
                    "awslogs-stream-prefix": "demo-app"
                }
            }
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::#AWSACCOUNTID#:role/ECSTaskExecutionRole"
}
