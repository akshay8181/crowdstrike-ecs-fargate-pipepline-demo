{
    "family": "java_repo",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "java",
            "image": "#AWSACCOUNTID#.dkr.ecr.us-east-1.amazonaws.com/java_repo:latest",
            "portMappings": [
                {
                    "containerPort": 8443,
                    "hostPort": 8443,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/java_repo",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::#AWSACCOUNTID#:role/ECSTaskExecutionRole",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
